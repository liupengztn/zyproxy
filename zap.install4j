                            <string>import java.nio.file.DirectoryStream;
import java.nio.file.Files;
import java.nio.file.Path;
import java.nio.file.Paths;
import java.nio.file.attribute.PosixFileAttributes;
import java.nio.file.attribute.PosixFilePermission;

Path file = Paths.get((String) context.getVariable("sys.contentDir"), "Contents", "Java", "zap.sh");
Set&lt;PosixFilePermission&gt; perms = Files.readAttributes(file, PosixFileAttributes.class).permissions();
perms.add(PosixFilePermission.OWNER_EXECUTE);
perms.add(PosixFilePermission.OTHERS_EXECUTE);
perms.add(PosixFilePermission.GROUP_EXECUTE);
try {
    Files.setPosixFilePermissions(file, perms);
    Util.logInfo(null, "Set execute permissions to: " + file);
} catch (IOException e) {
    context.handleCriticalException(e);
    return false;
}

file = Paths.get((String) context.getVariable("sys.contentDir"), "Contents", "MacOS", "OWASP ZAP.sh");
perms = Files.readAttributes(file, PosixFileAttributes.class).permissions();
perms.add(PosixFilePermission.OWNER_EXECUTE);
perms.add(PosixFilePermission.OTHERS_EXECUTE);
perms.add(PosixFilePermission.GROUP_EXECUTE);
try {
    Files.setPosixFilePermissions(file, perms);
    Util.logInfo(null, "Set execute permissions to: " + file);
} catch (IOException e) {
    context.handleCriticalException(e);
    return false;
}

Path binJavaDir = Paths.get((String) context.getVariable("sys.contentDir"), "Contents", "PlugIns", context.getCompilerVariable("macOsJreDirName"), "Contents", "Home", "bin");
Util.logInfo(null, "Setting execute permissions to files in: " + binJavaDir);
try (DirectoryStream&lt;Path&gt; stream = Files.newDirectoryStream(binJavaDir)) {
    for (Path binFile : stream) {
        perms = Files.readAttributes(binFile, PosixFileAttributes.class).permissions();
        perms.add(PosixFilePermission.OWNER_EXECUTE);
        perms.add(PosixFilePermission.OTHERS_EXECUTE);
        perms.add(PosixFilePermission.GROUP_EXECUTE);
        try {
            Files.setPosixFilePermissions(binFile, perms);
            Util.logInfo(null, "Set execute permissions to: " + binFile);
        } catch (IOException e) {
            context.handleCriticalException(e);
            return false;
        }
    }
}

return true;</string>
